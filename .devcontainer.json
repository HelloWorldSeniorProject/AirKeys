{
    "name": "AirKeys",
    "dockerComposeFile": "docker-compose.yml",
    "service": "python",
    "shutdownAction": "stopCompose",
    "workspaceFolder": "/home/NRuser/airkeys/",
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python",
				"VisualStudioExptTeam.vscodeintellicode",
				"KevinRose.vsc-python-indent",
				"terrastruct.d2"
			]
		}
	},
    "postCreateCommand": "dos2unix scripts/* && chmod +x scripts/init.sh && scripts/init.sh"
}